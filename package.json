{
  "name": "cw-components",
  "version": "1.9.0",
  "description": "CoverWallet components",
  "main": "./build/lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/coverwallet/cw-components.git"
  },
  "homepage": "https://github.com/coverwallet/cw-components",
  "bugs": "https://github.com/coverwallet/cw-components/issues",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "postinstall": "npm run build",
    "test": "rackt test --single-run --browsers Firefox",
    "start": "npm run build & webpack-dev-server --inline --content-base examples/ --port 8002",
    "build": "babel -d build/lib ./lib",
    "build-watch": "npm run styleguide-fonts & babel -w -d build/lib ./lib",
    "copy-fonts": "cp -R node_modules/cw-styleguide/fonts examples/basic",
    "copy-images": "cp -R node_modules/cw-styleguide/images examples/basic",
    "styleguide-fonts": "npm run copy-fonts & npm run copy-images",
    "gh-pages": "./scripts/gh-pages.sh",
    "bower": "rackt build"
  },
  "authors": [
    "CoverWallet"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "cw-styleguide": "git+https://github.com/coverwallet/styleguide.git#v1.9.5",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.10.0",
    "node-sass": "^3.4.2",
    "rackt-cli": "^0.4.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "tags": [
    "React",
    "Component",
    ""
  ],
  "keywords": [
    "React",
    "Component",
    ""
  ],
  "dependencies": {
    "classnames": "^2.2.3",
    "smoothscroll": "^0.2.2"
  }
}
